{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang++ build active file",
            "command": "/usr/bin/clang++",
            "args": [
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-std=gnu++14",
                "-stdlib=libc++",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-L${workspaceFolder}/src/deps/yoga/lib/",
                "-llibyoga",
                "-I/usr/local/include/",
                "-I${workspaceFolder}/src/deps/yoga/include/",
                "-I/usr/local/Cellar/python@3.9/3.9.14/Frameworks/Python.framework/Versions/3.9/include/python3.9/",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}